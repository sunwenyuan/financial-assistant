{
  "env": {
    "es6": true,
    "browser": true
  },
  "extends": "airbnb",
  "rules": {
    "no-console": 0,
    "no-unused-vars": 1,
    "max-len": 0,
    "comma-dangle": [0],
    "quotes": [0],
    "new-cap": 0,
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
    "react/prefer-stateless-function": [0, { "ignorePureComponents": true}],
    "react/prop-types": [0],
    "react/forbid-prop-types": [0],
    "react/no-unescaped-entities": 0
  }
}
